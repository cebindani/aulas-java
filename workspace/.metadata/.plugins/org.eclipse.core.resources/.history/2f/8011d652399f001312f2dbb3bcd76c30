package helloworld;

public class Hello {

	public static void main(String[] args) {
		
		boolean a = true;
		boolean b = true;
		boolean c = false;
		
		System.out.println("Valor: "+ (a && b));
		System.out.println("Valor: "+ (a && c));
		System.out.println("Valor: "+ (a || b));
		System.out.println("Valor: "+ (a || c));
		System.out.println("Valor: "+ (!a));
		System.out.println("Valor: "+ (!(a && b)));
		System.out.println("Valor: "+ (!a || !b));
		

	}

}
