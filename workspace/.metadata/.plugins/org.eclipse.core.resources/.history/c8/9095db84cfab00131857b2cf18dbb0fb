package helloworld;

public class Retangulo {
	Ponto esquerdoSuperior;
	Ponto direitoInferior;

	public Retangulo(Ponto esqSup, Ponto dirInf) {
		// TODO Auto-generated constructor stub
		this.esquerdoSuperior = esqSup;
		this.direitoInferior = dirInf;
	}

	double area() {
		return (direitoInferior.x - esquerdoSuperior.x)
				* (esquerdoSuperior.y - direitoInferior.y) / 2.0;
	}
}
